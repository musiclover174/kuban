html,
body
  font-family $os
body
  overflow-x hidden
  &:not(.pace-done)
    overflow hidden
      
.container
  w 100%
  max-width 1170px
  margin 0 auto
    
.content
  h1:not([class])
    color $dark
    font-family $slab
    fontify 48px 700 53px
    margin 43px 0 35px
  h2:not([class])
    color $dark
    font-family $slab
    fontify 36px 700 43px
    margin 43px 0 28px
  h3:not([class])
    color $dark
    font-family $slab
    fontify 30px 700 37px
    margin 43px 0 31px
  h4:not([class])
    color $dark
    font-family $slab
    fontify 24px 700 33px
    margin 43px 0 17px
  img:not([class])
    margin 16px 0
    max-width 100%
  ul:not([class])
    list-style  none
    margin  18px 0
  ul:not([class]) li
    pl 30px
    pos relative
    fontify 16px 300 24px
  ul:not([class]) li + li
    mt 16px
  ul:not([class]) > li:before
    content ""
    pos absolute
    l 0
    t 7px
    size 7px
    border-radius 50%
    bgc $blue
  ol:not([class])
    list-style none
    margin 18px 0 30px
    counter-reset myCounter
  ol:not([class]) li
    pl 30px
    pos relative
    fontify 16px 300 24px
  ol:not([class]) li + li
    mt 16px
  ol:not([class]) > li:before
    counter-increment myCounter
    content counter(myCounter)"."	
    pos absolute
    l 0
    t 0
    w 30px
    pr 9px
    color $blue
    fontify 16px 700 24px
  p:not([class])
    color $dark
    fontify 16px 300 24px
    margin 18px 0
  a:not([class])
    color $blue
    border-bottom 1px solid alpha($blue, .4)
    transition .5s border
    &:hover
      border-color transparent
 
  *:not([class]):not(span):first-child
    mt 0 !important
  *:not([class]):not(span):last-child
    margin-bottom 0 !important

  table
    th
      text-align left
      padding 18px 24px
      color white
      fontify 16px 300 24px
      bgc $blue
    td
      padding 14px 24px
      color $dark
      fontify 16px 300 24px
      border 1px solid #e8ebf5
    tbody
      border 2px solid #e8ebf5
      border-top 0

input::-webkit-input-placeholder
  color alpha(white, .5)
input::-moz-placeholder
  color alpha(white, .5)
input:-ms-input-placeholder
  color alpha(white, .5)
    
div[class\^="table-scroller"]
  overflow auto
  pos relative
  pl 1px
  &:not(:last-child)
    mb 36px
  @media $desktopMax
    w 100%
    
.common
  &__input
    w 100%
    h 36px
    border 0
    border-bottom 1px solid alpha(white, .3)
    bgc transparent
    color white
    font-family $os
    font-size 16px
    line-height 300
    transition .7s border
    &.notempty, 
    &:focus
      border-bottom 1px solid alpha(white, 1)
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      t -17px
      line-height 18px
  &__textarea
    w 100%
    h 76px
    overflow hidden
    resize none
    padding 0 0 11px
    border 0
    border-bottom 1px solid alpha(white, .3)
    bgc transparent
    color white
    font-family $os
    fontify 16px 300 22px
    transition .7s border
    & + ^[0]__placeholder
      line-height 22px
    &.notempty, 
    &:focus
      border-bottom 1px solid alpha(white, 1)
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      t -17px
      line-height 18px
  &__sbm
    cursor pointer
    font-family $os
    mt 5px
    border-radius 5px
  &__select
    h 36px
    bgc #fff
    border 2px solid #b6b6b6
    w auto
    cursor pointer
    transition .5s border
    &.warning
      border-color #ed1c24
  &__placeholder
    pos absolute
    z-index -1
    l 0 
    t 0
    color alpha(white, .5)
    fontify 13px 400 35px
    letter-spacing .1em
    text-transform uppercase
    text-overflow ellipsis
    white-space nowrap
    overflow hidden
    max-width 100%
    transition .7s top, .7s line-height
  &__col
    pos relative
    z-index 1
    w calc(50% - 30px)
    &_col3
      w calc(100% / 3 - 20px)
    &_wide
      w 100%
    @media $LandscapeMedium
      w 100%
      & + &
        mt 35px
  &__row
    display flex
    justify-content space-between
    pos relative
    w 100%
    @media $LandscapeMedium
      flex-direction column
    & + &
      mt 35px
  &__fileover
    w 100%
    pos relative
    transition .5s border
    &.warning
      border-color #ed1c24
    & + &
      mt 20px
  &__file
    pos absolute
    w 0
    h 0
    l 0
    t 0
    opacity 0
    &[data-req]:after
      content "*"
      pos absolute
      r -11px
      fontify 23px 400 14px
      t -3px
    &name
      ib()
      pos relative
      l 26px
      cursor pointer
      color alpha(white, .5)
      fontify 14px 700 14px
      letter-spacing .1em
      text-transform uppercase
      border-bottom 1px solid alpha(white, .25)
      transition .5s border
      @media $lDesktopSmall
        font-size 12px
      &.choosed
        color white
        text-transform none
        letter-spacing 0
        font-weight 300
      &.choosed,
      &:hover
        border-color transparent
      &:before
        content ""
        pos absolute
        l -24px
        t 3px
        size 16px 8px
        background url("data:image/png;base64,") no-repeat center
      &.choosed + ^[-1]delete
        visibility visible
        cursor pointer
      &.choosed:after
        content ""
    &delete
      ib()
      visibility hidden
      mt 3px
      ml 32px
      size 9px
      background url("data:image/png;base64,") no-repeat center
      
.fakecheckbox
  ib()
  pos relative
  &__label
    ib()
    color alpha(white, .5)
    fontify 14px 300 20px
    pos relative
    pl 30px
    pt 9px
    &:before 
      content ""
      pos absolute
      l 0px
      t 8px
      size 16px
      border 2px solid alpha(white, .5)
      border-radius 4px
      cursor pointer
      transition .5s border
    &:after
      content ""
      pos absolute
      size 10px 7px
      background url('data:image/png;base64,') no-repeat center
      l 5px
      t 15px
      opacity 0
      cursor pointer
      transition .5s opacity
    a
      color inherit
      border-bottom 1px solid alpha(white, .2)
      transition .7s border
      &:hover
        border-color transparent
  &__truecheck
    pos absolute
    l 0
    t 0
    w 0
    h 0
    border 0
    opacity 0
    &:checked + ^[0]__label:after
      opacity 1
    &.warning + ^[0]__label:before 
      border-color #ff0f37

input.warning,
textarea.warning
  border-color #ff0f37

.hidden-block
  display none